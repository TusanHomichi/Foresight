loadSuperClass('kdepackage')
class Decibel(kdePackageRecipe):

    name = 'decibel'
    version = '0.5.0'

    buildRequires = [
        'tapioca-qt:devel'
    ]

    def unpack(r):

        r.addArchive(
            'http://decibel.kde.org/fileadmin/downloads/decibel/releases/'
        )

