class Jsonrpclib(PackageRecipe):

    name = 'jsonrpclib'
    version = '0.0'

    buildRequires = ['python-setuptools:python','python:devel', 'python-simplejson:python']

    def setup(r):
        r.addArchive('jsonrpclib.tar.gz')
        r.PythonSetup()
