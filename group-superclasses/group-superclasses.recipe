class GroupSuperclasses(GroupRecipe):
    name = 'group-superclasses'
    version = '1'

    autoResolve = False
    depCheck = False

    def setup(r):
        r.add('autopackage')
        r.add('baserequires')
        r.add('buildpackage')
        r.add('cpackage')
        r.add('derivedpackage')
        r.add('fileset')
        r.add('group')
        r.add('groupinfo')
        r.add('package')
        r.add('redirect')
        r.add('userinfo')
