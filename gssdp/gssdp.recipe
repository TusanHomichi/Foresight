class Gssdp(AutoPackageRecipe):
    name = 'gssdp'
    version = '0.11.2'
 
    buildRequires = [
        'atk:devel',
        'cairo:devel',
        'fontconfig:devel',
        'freetype:devel',
        'glib:devel',
        'gtk-doc:runtime',
        'gtk:devel',
        'libsoup:devel',
        'pango:devel',
        'pkgconfig:devel',
        ]
 
    def unpack(r):
        r.addArchive('http://ftp.heanet.ie/mirrors/gnome/sources/gssdp/0.11/')


