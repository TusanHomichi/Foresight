loadSuperClass('pypipackage')
class PythonGcp(PyPiPackage):
    name = 'python-gcp'
    pyname = name.replace('python-', '')
    version = '0.1.3'
    buildRequires =  [
        'dbus-python:python',
        'pygobject:python',
        'python-progressbar:python',
    ]
